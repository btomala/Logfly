               </items>
            </ToolBar>
            <ToolBar nodeOrientation="RIGHT_TO_LEFT" prefHeight="40.0" prefWidth="200.0" style="-fx-background-color: black;" HBox.hgrow="ALWAYS">
               <items>
                  <Button fx:id="btClose" mnemonicParsing="false" onAction="#handleClose" text="Fermer" />
