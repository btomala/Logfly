                  <Label fx:id="lbSecteur" text="fSecteur" textFill="WHITE">
                     <padding>
                        <Insets right="10.0" />
                     </padding>
                  </Label>
                  <TextField fx:id="txLocality" />
                  <Button fx:id="btGo" mnemonicParsing="false" onAction="#handleGo" text="Go.." />
